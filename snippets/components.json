{
    "WB Component Photo": {
      "prefix": "wbs-comp-photo",
      "body": [
        ":photo{",
        "  src=\"${1:URL}\"",
        "  ${2:caption=\"\"}",
        "  ${3:width=\"300px\"}",
        "  ${4:float=\"${5|right,left,center|}\"}",
        "}"
      ],
      "description": "Компонент фото. caption можно не указывать",
      "attributes": {
        "src": {
          "description": "URL изображения",
          "default": ""
        },
        "caption": {
          "description": "Подпись к изображению",
          "default": ""
        },
        "width": {
          "description": "Ширина изображения (например, \"300px\" или \"50%\")",
          "default": "300px",
          "values": ["100px", "200px", "300px", "50%", "100%"]
        },
        "float": {
          "description": "Выравнивание изображения",
          "default": "center",
          "values": ["right", "left", "center"]
        }
      }
    },
    "WB Component Gallery": {
      "prefix": "wbs-comp-gallery",
      "body": [
        ":gallery{",
        "  :data='[",
        "    [\"${1:URL}\", \"${2:Подпись}\"],",
        "    [\"${3:URL}\", \"${4:Подпись}\"],",
        "    [\"${5:URL}\", \"${6:Подпись}\"],",
        "    [\"${7:URL}\", \"${8:Подпись}\"],",
        "    [\"${9:URL}\", \"${10:Подпись}\"],",
        "    [\"${11:URL}\", \"${12:Подпись}\"],",
        "    [\"${13:URL}\", \"${14:Подпись}\"],",
        "    [\"${15:URL}\", \"${16:Подпись}\"],",
        "    [\"${17:URL}\", \"${18:Подпись}\"],",
        "    [\"${19:URL}\", \"${20:Подпись}\"]$0",
        "  ]'",
        "}"
      ],
      "description": "Галерея изображений. Подпись можно не указывать.",
      "attributes": {
        "data": {
          "description": "JSON-массив данных для галереи",
          "default": "[]"
        }
      }
    },
    "WB Component Video": {
      "prefix": "wbs-comp-video",
      "body": [
        ":video-player{",
        "  url=\"${1:URL}\"",
        "  ${2:width=\"500px\"}",
        "  ${3:height=\"280px\"}",
        "  ${4:float=\"${5|left,right|}\"}",
        "  ${6:cover=\"\"}",
        "}"
      ],
      "description": "Компонент видеоплеера.",
      "attributes": {
        "url": {
          "description": "Ссылка на видео",
          "default": ""
        },
        "width": {
          "description": "Ширина плеера",
          "default": "500px",
          "values": ["300px", "400px", "500px", "100%"]
        },
        "height": {
          "description": "Высота плеера",
          "default": "280px",
          "values": ["200px", "280px", "360px"]
        },
        "float": {
          "description": "Выравнивание плеера",
          "default": "left",
          "values": ["left", "right", "center"]
        },
        "cover": {
          "description": "URL обложки видео",
          "default": ""
        }
      }
    },
    "WB Component Video Gallery": {
      "prefix": "wbs-comp-video-gallery",
      "body": [
        ":video-gallery{",
        "  :data='[",
        "    [\"${1:URL}\", \"${2:Подпись}\", \"${3:Обложка}\"],",
        "    [\"${4:URL}\", \"${5:Подпись}\", \"${6:Обложка}\"],",
        "    [\"${7:URL}\", \"${8:Подпись}\", \"${9:Обложка}\"],",
        "    [\"${10:URL}\", \"${11:Подпись}\", \"${12:Обложка}\"],",
        "    [\"${13:URL}\", \"${14:Подпись}\", \"${15:Обложка}\"],",
        "    [\"${16:URL}\", \"${17:Подпись}\", \"${18:Обложка}\"]$0",
        "  ]'",
        "}"
      ],
      "description": "Видеогалерея. Подпись можно не указывать.",
      "attributes": {
        "data": {
          "description": "JSON-массив данных для видеогалереи",
          "default": "[]"
        }
      }
    }
  }
  